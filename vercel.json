{
  "version": 2,
  "builds": [
    {
      "src": "app.js",
      "use": "@vercel/node"
    },
    {
      "src": "routes/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/reduce-ticket",
      "dest": "/routes/reduce-ticket.js"
    }
  ],
  "functions": {
    "routes/reduce-ticket.js": {
      "maxDuration": 60
    }
  }
}
